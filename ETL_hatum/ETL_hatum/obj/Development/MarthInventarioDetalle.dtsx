                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT matB.cantidad as Stock_producto,fecD.id_fecha,itemD.id_item,provD.id_proveedor,almB.Stock as Stock_materiaPrima,proD.id_producto,(matB.precio+matB.total_comprado+matB.cantidad) as sumas_totales
FROM hatum.dbo.Almacen almH		INNER JOIN hatum.dbo.Almacen almB ON(almH.id_almacen=almB.id_almacen)
								INNER JOIN hatum.dbo.producto proB ON(proB.id_almacen=almH.id_almacen)
								INNER JOIN hatum.dbo.materia_prima matB ON(almH.id_almacen=matB.id_almacen)
								Inner JOIN hatum.dbo.proveedores prveeB ON(matB.id_provedoor=prveeB.id_proveedor)
								INNER JOIN DataMarth_Detalle_inventario.dbo.[D-_item] itemD ON(itemD.ref_materia=matB.id_materia)
								INNER JOIN DataMarth_Detalle_inventario.dbo.D_proveedor provD ON(provD.Ref_proveedor=prveeB.id_proveedor)
								INNER JOIN DataMarth_Detalle_inventario.dbo.D_productos proD ON(proD.ref_producto=proB.id_producto)
								INNER JOIN DataMarth_Detalle_inventario.dbo.D_tiempo fecD ON(fecD.fecha=almH.fecha)
GROUP BY almB.Stock,fecD.id_fecha,itemD.id_item,provD.id_proveedor,proD.id_producto,matB.cantidad,matB.precio+matB.total_comprado+matB.cantidad</property>
